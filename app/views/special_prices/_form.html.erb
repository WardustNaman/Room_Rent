<%= form_for([@room, @special_price]) do |form| %>
  <div class="field">
    <%= form.label :price %>
    <%= form.number_field :price, id: :special_price_price %>
  </div>

  <div class="field">
    <%= form.label :start_date %>
    <%= form.date_select :start_date, id: :special_price_start_date %>
  </div>

  <div class="field">
    <%= form.label :end_date %>
